{
  "name": "@keywork/root",
  "license": "AGPL-3.0",
  "version": "0.1.0",
  "private": true,
  "description": "A collection of useful tools for building web apps on Cloudflare Workers.",
  "keywords": [
    "keywork",
    "cloudflare",
    "workers",
    "cloudflare workers",
    "kv",
    "cloudflare kv",
    "key value",
    "database",
    "nosql",
    "react"
  ],
