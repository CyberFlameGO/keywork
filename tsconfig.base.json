{
  "compilerOptions": {
    "module": "esnext",
    "target": "esnext",
    "lib": [
      "esnext"
    ],
    "paths": {
      "keywork": ["packages/keywork/index.ts"],
      "@keywork/utils": ["packages/utils/index.ts"],
      "@keywork/collections": ["packages/collections/index.ts"],
      "@keywork/react-isomorphic": ["packages/react-isomorphic/index.ts"]
    },
    "types": [],
    "jsx": "react",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "isolatedModules": true,
    "experimentalDecorators": true,
    "baseUrl": ".",
    "sourceMap": true,
    "declaration": true,
    "declarationMap": true,
    "composite": true,
    "emitDeclarationOnly": true,
    "checkJs": true,
    "allowJs": true
  },
  "exclude": [
    "**/dist/**/*"
  ]
}
